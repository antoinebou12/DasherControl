{
  "name": "views",
  "description": "vue js views",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.config.js --progress --profile",
    "build": "webpack serve --mode production --config webpack.config.js --progress --profile",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src tests/unit"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "postcss-import": "^14.0.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.5.16",
    "vue-grid-layout": "^2.3.12",
    "vue-router": "^3.1.2",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 8"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/no-babel",
    "transform": {
      "^.+\\.vue$": "vue-jest"
    }
  },
  "engines": {
    "node": "^14"
  }
}
